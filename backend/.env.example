# Database Configuration (for Render deployment)
DATABASE_URL=jdbc:mysql://username:password@host:port/database
DB_USERNAME=root
DB_PASSWORD=your_secure_password

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here_minimum_256_bits
JWT_EXPIRATION=86400000

# Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-flash
GEMINI_MAX_TOKENS=2048
GEMINI_TEMPERATURE=0.7

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:5173

# Server Configuration (Render uses PORT environment variable)
PORT=8080